{
    "name": "kryptopus-candlestick-storage-elasticsearch",
    "description": "Elasticsearch storage for candlesticks",
    "version": "1.0.0",
    "author": "neolao <contact@neolao.com>",
    "repository": "https://github.com/kryptopus/candlestick-storage-elasticsearch.git",
    "license": "MIT",
    "files": [
        "src",
        "lib"
    ],
    "main": "lib/Bundle.js",
    "engines": {
        "node": ">= 9"
    },
    "dependencies": {
        "decko": "^1.2.0",
        "elasticsearch": "^14.2.1",
        "kryptopus-candlestick": "^1.0.1",
        "kryptopus-candlestick-storage": "^1.1.1",
        "solfegejs-cli": "^4.0.2"
    },
    "devDependencies": {
        "babel-cli": "^6.7",
        "babel-preset-solfege": "^1.2.2",
        "eslint": "^3.19",
        "eslint-plugin-solfege": "^1.0",
        "flow-bin": "^0.60.1",
        "solfegejs-application": "^3.0.1",
        "solfegejs-dependency-injection": "^2.2.5",
        "watch": "^1.0.2"
    },
    "scripts": {
        "flow-stop": "flow stop",
        "flow-start": "flow start",
        "flow-restart": "flow stop && flow start",
        "lint": "eslint --ext .js src",
        "prepare": "npm run lint && npm run build",
        "prebuild": "npm run lint && flow",
        "postbuild": "echo \"---------- READY ---------\"",
        "build": "babel --out-dir lib src --copy-files",
        "watch": "watch 'echo \"----- START BUILDING -----\"; npm run build --silent' ./src",
        "example-build": "flow; babel --out-dir example/lib example/src --copy-files",
        "example-console": "node example/lib/console.js"
    },
    "babel": {
        "presets": [
            "solfege"
        ],
        "comments": false
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:solfege/recommended"
        ]
    }
}
